<% if @list_questions %>
  <%= turbo_stream.update "list_questions" do %>
    <%= render partial: "users/list_questions", locals: { list_questions: @list_questions, list_id: @list.id } %>
  <% end %>
<% else %>
  <%= render 'update_favorite_and_liked_count', question: @question, context: @context, game_records: @game_records %>
<% end %>

<%= turbo_stream_flash %>